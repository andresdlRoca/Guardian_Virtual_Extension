{
  "manifest_version": 3,
  "name": "Guardian Virtual",
  "version": "1.0",
  "description": "Phishing Detection Extension",
  "icons": {
      "128": "icon128.png"
  },
  "action": {
      "default_popup": "index.html"
  },

  "background": {
      "service_worker": "service-worker.js"
  },

  "permissions": [
      "contextMenus",
      "activeTab",
      "scripting"
  ]
}